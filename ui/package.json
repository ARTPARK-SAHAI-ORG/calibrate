{
  "name": "calibrate-ui",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "tsx source/cli.tsx",
    "build": "tsc",
    "bundle": "esbuild source/cli.tsx --bundle --platform=node --target=node18 --outfile=../calibrate/ui/cli.bundle.mjs --format=esm --banner:js=\"import{createRequire}from'module';const require=createRequire(import.meta.url);\"",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "react-devtools-core": "^4.28.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "esbuild": "^0.27.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
